{"ast":null,"code":"import axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"BankManager\",\n  data() {\n    return {\n      banks: [],\n      searchQuery: \"\"\n    };\n  },\n  computed: {\n    ...mapGetters([\"getToken\"]),\n    filteredBanks() {\n      if (!this.searchQuery) return this.banks;\n      const q = this.searchQuery.toLowerCase();\n      return this.banks.filter(bank => (bank.accountName || \"\").toLowerCase().includes(q) || (bank.accountNumber || \"\").toLowerCase().includes(q) || (bank.bankName || \"\").toLowerCase().includes(q) || (bank.user || \"\").toLowerCase().includes(q));\n    }\n  },\n  mounted() {\n    this.fetchBanks();\n  },\n  methods: {\n    async fetchBanks() {\n      try {\n        const res = await axios.get(\"https://event-ticket-qa70.onrender.com/api/all-banks\");\n        this.banks = res.data.banks;\n      } catch (err) {\n        console.error(\"Error fetching banks:\", err);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","banks","searchQuery","computed","filteredBanks","q","toLowerCase","filter","bank","accountName","includes","accountNumber","bankName","user","mounted","fetchBanks","methods","res","get","err","console","error"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\app\\admin\\bank.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-4\">\r\n    <!-- ðŸ” Search -->\r\n    <div class=\"filter mb-3\">\r\n      <label for=\"searchInput\" class=\"form-label\">Search:</label>\r\n      <input\r\n        id=\"searchInput\"\r\n        v-model=\"searchQuery\"\r\n        type=\"text\"\r\n        placeholder=\"Search by account name, number, bank, or user\"\r\n        class=\"form-control\"\r\n      />\r\n    </div>\r\n\r\n    <!-- âœ… Bank Table -->\r\n    <div class=\"table-container\">\r\n      <table class=\"table custom-table table-striped mb-0\" v-if=\"filteredBanks.length\">\r\n        <thead>\r\n          <tr>\r\n            <th>Bank Account Name</th>\r\n            <th>Account Number</th>\r\n            <th>Bank Name</th>\r\n            <th>User</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody class=\"scrollable-tbody\">\r\n          <tr v-for=\"bank in filteredBanks\" :key=\"bank._id\">\r\n            <td>{{ bank.accountName }}</td>\r\n            <td>{{ bank.accountNumber }}</td>\r\n            <td>{{ bank.bankName }}</td>\r\n            <td>{{ bank.user }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n\r\n      <!-- âŒ No Results -->\r\n      <div v-else class=\"text-center text-muted mt-4\">No banks found.</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"BankManager\",\r\n  data() {\r\n    return {\r\n      banks: [],\r\n      searchQuery: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n    filteredBanks() {\r\n      if (!this.searchQuery) return this.banks;\r\n\r\n      const q = this.searchQuery.toLowerCase();\r\n\r\n      return this.banks.filter(\r\n        (bank) =>\r\n          (bank.accountName || \"\").toLowerCase().includes(q) ||\r\n          (bank.accountNumber || \"\").toLowerCase().includes(q) ||\r\n          (bank.bankName || \"\").toLowerCase().includes(q) ||\r\n          (bank.user || \"\").toLowerCase().includes(q)\r\n      );\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchBanks();\r\n  },\r\n  methods: {\r\n    async fetchBanks() {\r\n      try {\r\n        const res = await axios.get(\r\n          \"https://event-ticket-qa70.onrender.com/api/all-banks\"\r\n        );\r\n        this.banks = res.data.banks;\r\n      } catch (err) {\r\n        console.error(\"Error fetching banks:\", err);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.custom-table {\r\n  width: 100%;\r\n  border-collapse: collapse