  class=\"card-img-top\"\r\n              style=\"height: 200px; object-fit: cover\"\r\n            />\r\n\r\n            <!-- Card Body -->\r\n            <div class=\"card-body\">\r\n              <h5 class=\"card-title\">{{ product.productID.title }}</h5>\r\n\r\n              <!-- Event Info -->\r\n              <div class=\"border-top pt-2 mt-2\">\r\n                <h6 class=\"fw-bold\">Event Info</h6>\r\n                <p class=\"mb-1\">\r\n                  <strong>Location:</strong> {{ product.productID.event.location.name }}\r\n                </p>\r\n                <p class=\"mb-1\">\r\n                  <strong>Start:</strong>\r\n                  {{ formatDate(product.productID.event.start) }} at\r\n                  {{ product.productID.event.startTime }}\r\n                </p>\r\n                <p class=\"mb-1\">\r\n                  <span v-if=\"product.productID.event.end\"\r\n                    >- {{ formatDate(product.productID.event.end) }} at\r\n                  </span>\r\n                </p>\r\n              </div>\r\n\r\n              <!-- Tickets -->\r\n              <div class=\"border-top pt-2 mt-2\">\r\n                <h6 class=\"fw-bold\">Tickets</h6>\r\n                <ul class=\"list-unstyled mb-0\">\r\n                  <li\r\n                    v-for=\"ticket in product.productID.event.tickets\"\r\n                    :key=\"ticket._id\"\r\n                    class=\"d-flex justify-content-between\"\r\n                  >\r\n                    <span>{{ ticket.name }}</span>\r\n                    <span class=\"text-muted\">\r\n                      ${{ ticket.price }} ({{ ticket.quantity }} left)\r\n                    </span>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Scanner Section -->\r\n        <div class=\"col-md-4\">\r\n          <div class=\"card shadow-sm border-0 h-100\">\r\n            <div class=\"text center bg-success p-2\" style=\"color: white\">\r\n              <h5 class=\"mb-0\">Scanner</h5>\r\n            </div>\r\n            <div class=\"card-body d-flex align-items-center justify-content-center\">\r\n              <Scanner />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Scanner from \"./app/admin/scanner.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    Scanner,\r\n  },\r\n  data() {\r\n    return {\r\n      products: [],\r\n      loading: true,\r\n    };\r\n  },\r\n  async mounted() {\r\n    try {\r\n      const token = localStorage.getItem(\"staffToken\");\r\n      console.log(token);\r\n      const res = await axios.get(\"https://event-ticket-backend-gnmw.onrender.com/api/staff\", {\r\n        headers: {\r\n          Authorization: `Bearer ${token}`,\r\n        },\r\n      });\r\n      console.log(token);\r\n      this.products = res.data.products; // get the nested products array\r\n    } catch (err) {\r\n      console.error(\"Error fetching staff products:\", err);\r\n    } finally {\r\n      this.loading = false;\r\n    }\r\n  },\r\n  methods: {\r\n    formatDate(dateStr) {\r\n      const date = new Date(dateStr);\r\n      return date.toLocaleDateString();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  padding: 20px;\r\n}\r\n.product-card {\r\n  border: 1px solid #ddd;\r\n  padding: 15px;\r\n  margin-bottom: 15px;\r\n  border-radius: 6px;\r\n  background: #fafafa;\r\n}\r\n.product-img {\r\n  max-width: 200px;\r\n  display: block;\r\n  margin: 10px 0;\r\n}\r\n</style>\r\n"],"mappings":"AAkFA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,yBAAyB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC;MAChDC,OAAO,CAACC,GAAG