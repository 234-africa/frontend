{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  components: {},\n  data() {\n    return {\n      selectedStatus: \"\",\n      // Selected filter status\n      searchQuery: \"\",\n      // Search query\n      currentPage: 1,\n      // Current page number\n      rowsPerPage: 10,\n      // Number of rows per page\n      totalPrice: 0,\n      orders: []\n    };\n  },\n  computed: {\n    ...mapGetters([\"getToken\", \"getProductsByUser\"]),\n    productsByUser() {\n      return this.getProductsByUser;\n    }\n  },\n  methods: {\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    changePage(page) {\n      this.currentPage = page;\n    },\n    applyFilters() {\n      this.currentPage = 1; // Reset to the first page when applying filters\n    }\n  },\n\n  mounted() {\n    axios.get(\"http://localhost:4000/api/orders\", {\n      headers: {\n        Authorization: `Bearer ${this.getToken}`\n      }\n    }).then(response => {\n      this.orders = response.data.orders;\n      console.log(\"Orders fetched:\", this.orders);\n      const total = this.orders.reduce((sum, order) => sum + order.price, 0);\n      this.totalPrice = total;\n      console.log(\"Total price:\", this.totalPrice);\n    }).catch(error => {\n      console.error(\"Error fetching orders:\", error.response?.data || error);\n    });\n    this.$store.dispatch(\"fetchProductsByUser\");\n  }\n};","map":{"version":3,"names":["mapGetters","axios","components","data","selectedStatus","searchQuery","currentPage","rowsPerPage","totalPrice","orders","computed","productsByUser","getProductsByUser","methods","prevPage","nextPage","totalPages","changePage","page","applyFilters","mounted","get","headers","Authorization","getToken","then","response","console","log","total","reduce","sum","order","price","catch","error","$store","dispatch"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\dashboard\\dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"container py-4\">\r\n    <h2 class=\"mb-4 text-center\">Dashboard Overview</h2>\r\n\r\n    <!-- Metrics Row -->\r\n    <div class=\"row g-3\">\r\n      <div class=\"col-md-4\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          {{ getProductsByUser.length }}\r\n\r\n          <p class=\"mb-1 fw-bold\">Total Events</p>\r\n          <small>{{ totalPercentage }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-md-4\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          {{ totalPrice }}\r\n          <p class=\"mb-1 fw-bold\">Total Revenue</p>\r\n          <small>{{ totalOrdersPercentage }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"col-md-4\">\r\n        <div class=\"p-3 border rounded text-center bg-light\">\r\n          {{ orders.length }}\r\n          <p class=\"mb-1 fw-bold\">Total Orders</p>\r\n          <small>{{ totalPercent }}% (30 days)</small>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"container-flui\">\r\n      <div class=\"form-head d-flex mb-3 align-items-start\">\r\n        <div class=\"mr-auto d-none d-lg-block\">\r\n          <h2 class=\"text-black font-w600 mb-0\">Orders</h2>\r\n          <p class=\"mb-0\">Here is your order list data</p>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <div class=\"filter row align-items-center\">\r\n          <div class=\"col-auto\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              v-model=\"searchQuery\"\r\n              placeholder=\"Search by Order ID\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center bg-success text-white p-2\">\r\n          <h5 class=\"mb-0\">My orders</h5>\r\n        </div>\r\n        <div class=\"card-body p-0\">\r\n          <div class=\"table-container\">\r\n            <table class=\"table table-striped mb-0\">\r\n              <thead>\r\n                <tr>\r\n                  <th>Order ID</th>\r\n                  <th>Date</th>\r\n                  <th>reference</th>\r\n                  <th>name of event</th>\r\n                  <th>Customer Name</th>\r\n                  <th>Phone number</th>\r\n                  <th>Amount</th>\r\n\r\n                  <th>Ticket type</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody class=\"\">\r\n                <tr v-for=\"(order, index) in orders\" :key=\"order._id\">\r\n                  <td>{{ order._id }}</td>\r\n                  <td>{{ new Date(order.createdAt).toLocaleString() }}</td>\r\n\r\n                  <td class=\"tes\">{{ order.reference }}</td>\r\n                  <td>{{ order.title }}</td>\r\n                  <td>{{ order.contact?.email || \"N/A\" }}</td>\r\n                  <td>{{ order.contact?.phone || \"N/A\" }}</td>\r\n                  <td>${{ order.price }}</td>\r\n                  <td>\r\n                    <div v-for=\"ticket in order.tickets\" :key=\"ticket._id\">\r\n                      {{ ticket.name }} x {{ ticket.quantity }}\r\n                    </div>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"pagination\">\r\n            <button\r\n              class=\"pagination-btn\"\r\n              :disabled=\"currentPage === 1\"\r\n              @click=\"prevPage\"\r\n            >\r\n              Previous\r\n            </button>\r\n            <button\r\n              v-for=\"page in totalPages\"\r\n              :key=\"page\"\r\n              class=\"pagination-btn\"\r\n              :class=\"{ active: currentPage === page }\"\r\n              @click=\"changePage(page)\"\r\n            >\r\n              {{ page }}\r\n            </button>\r\n            <button\r\n              class=\"pagination-btn\"\r\n              :disabled=\"currentPage === totalPages\"\r\n              @click=\"nextPage\"\r\n            >\r\n              Next\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: {},\r\n  data() {\r\n    return {\r\n      selectedStatus: \"\", // Selected filter status\r\n      searchQuery: \"\", // Search query\r\n      currentPage: 1, // Current page number\r\n      rowsPerPage: 10, // Number of rows per page\r\n      totalPrice: 0,\r\n      orders: [],\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"getToken\", \"getProductsByUser\"]),\r\n\r\n    productsByUser() {\r\n      return this.getProductsByUser;\r\n    },\r\n  },\r\n  methods: {\r\n    prevPage() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;\r\n      }\r\n    },\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;\r\n      }\r\n    },\r\n    changePage(page) {\r\n      this.currentPage = page;\r\n    },\r\n    applyFilters() {\r\n      this.currentPage = 1; // Reset to the first page when applying filters\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:4000/api/orders\", {\r\n        headers: {\r\n          Authorization: `Bearer ${this.getToken}`,\r\n        },\r\n      })\r\n      .then((response) => {\r\n        this.orders = response.data.orders;\r\n\r\n        console.log(\"Orders fetched:\", this.orders);\r\n        const total = this.orders.reduce((sum, order) => sum + order.price, 0);\r\n        this.totalPrice = total;\r\n\r\n        console.log(\"Total price:\", this.totalPrice);\r\n      })\r\n      .catch((error) => {\r\n        console.error(\"Error fetching orders:\", error.response?.data || error);\r\n      });\r\n\r\n    this.$store.dispatch(\"fetchProductsByUser\");\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.tes {\r\n  text-transform: none !important;\r\n}\r\n.custom-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.custom-table th,\r\n.custom-table td {\r\n  padding: 10px;\r\n  text-align: left;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.custom-table th {\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n.custom-table td button {\r\n  padding: 5px 10px;\r\n  font-size: 14px;\r\n  border-radius: 4px;\r\n}\r\n\r\n.custom-table td .dropdown-menu {\r\n  margin-top: 5px;\r\n}\r\n\r\n.custom-table td .dropdown-menu button {\r\n  width: 100%;\r\n  text-align: left;\r\n}\r\n.pagination {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 10px;\r\n}\r\n\r\n.pagination button {\r\n  border: 1px solid #ccc;\r\n  background-color: #fff;\r\n  color: #000;\r\n  padding: 5px 10px;\r\n  margin: 0 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.pagination button.active {\r\n  background-color: #f4a213;\r\n  color: #fff;\r\n}\r\n\r\n.filter {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.filter label {\r\n  margin-right: 5px;\r\n}\r\n.table-container {\r\n  overflow-x: auto;\r\n}\r\n\r\n.scrollable-tbody {\r\n  display: block;\r\n  white-space: nowrap;\r\n}\r\n</style>\r\n"],"mappings":"AAuHA,SAASA,UAAS,QAAS,MAAM;AAEjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE,CAAC,CAAC;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,EAAE;MAAE;MACpBC,WAAW,EAAE,EAAE;MAAE;MACjBC,WAAW,EAAE,CAAC;MAAE;MAChBC,WAAW,EAAE,EAAE;MAAE;MACjBC,UAAU,EAAE,CAAC;MACbC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACR,GAAGV,UAAU,CAAC,CAAC,UAAU,EAAE,mBAAmB,CAAC,CAAC;IAEhDW,cAAcA,CAAA,EAAG;MACf,OAAO,IAAI,CAACC,iBAAiB;IAC/B;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACR,WAAU,GAAI,CAAC,EAAE;QACxB,IAAI,CAACA,WAAW,EAAE;MACpB;IACF,CAAC;IACDS,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI