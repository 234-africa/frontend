{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters, mapMutations } from \"vuex\";\nimport axios from \"axios\";\nimport paystack from \"vue-paystack\";\nimport QrcodeVue from \"qrcode.vue\";\nimport spinner from \"./spinner.vue\";\nimport { ref } from \"vue\";\nimport { getChartByID } from \"apexcharts\";\nexport default {\n  components: {\n    paystack,\n    QrcodeVue,\n    spinner\n  },\n  data() {\n    return {\n      currentStep: 0,\n      timer: \"10:00\",\n      spinner: false,\n      reference: \"\",\n      showQRCode: false,\n      countdown: null,\n      full_name: \"\",\n      publicKey: process.env.VUE_APP_PUBLIC_KEY,\n      steps: [\"Tickets\", \"Contact\", \"Payment\"],\n      tickets: [],\n      promoCode: \"\",\n      discountResult: null,\n      contact: {\n        firstName: \"\",\n        lastName: \"\",\n        email: \"\",\n        confirmEmail: \"\",\n        phone: \"\"\n      },\n      paymentMethod: \"\",\n      termsAccepted: false,\n      subscribe: false\n    };\n  },\n  computed: {\n    // 1. Raw subtotal (tickets only, no service, no discount)\n    rawSubtotal() {\n      const subtotal = this.getCart.reduce((sum, product) => {\n        const ticketTotal = product.event.tickets.reduce((innerSum, ticket) => {\n          return innerSum + (ticket.selectedQuantity || 0) * (ticket.price || 0);\n        }, 0);\n        return sum + ticketTotal;\n      }, 0);\n      console.log(\"Raw Subtotal:\", subtotal);\n      return subtotal;\n    },\n    // 2. Service charge (based on raw subtotal)\n    serviceCharge() {\n      const charge = this.rawSubtotal * 0.075;\n      console.log(\"Service Charge (7.5%):\", charge);\n      return +charge.toFixed(2);\n    },\n    // 3. Subtotal before discount (subtotal + service charge)\n    subTotal() {\n      const subtotal = this.rawSubtotal + this.serviceCharge;\n      console.log(\"Subtotal Before Discount:\", subtotal);\n      return +subtotal.toFixed(2);\n    },\n    // 4. Discount amount (applied on subtotal before discount)\n    discountAmount() {\n      if (!this.discountResult) return 0;\n      let discount = 0;\n      if (this.discountResult.discountType === \"percentage\") {\n        discount = this.subTotal * (this.discountResult.discountValue / 100);\n      } else if (this.discountResult.discountType === \"flat\") {\n        discount = this.discountResult.discountValue;\n      }\n      console.log(\"Discount Applied:\", discount);\n      return +discount.toFixed(2);\n    },\n    // 5. Final total (subtotal - discount)\n    cartTotal() {\n      const total = this.subTotal - this.discountAmount;\n      console.log(\"Final Cart Total:\", total);\n      return +total.toFixed(2);\n    },\n    email() {\n      return this.getContactInfo.email;\n    },\n    amount() {\n      return this.cartTotal;\n    },\n    steps() {\n      // If the cart has a total greater than 0, show payment step\n      return this.cartTotal > 0 ? [\"Tickets\", \"Contact\", \"Payment\"] : [\"Tickets\", \"Contact\"];\n    },\n    ...mapGetters([\"getCart\", \"getContactInfo\", \"getSelectedTickets\", \"getCartTotal\"]),\n    selectedTickets() {\n      return this.getCart.flatMap(product => {\n        return product.event?.tickets?.filter(ticket => ticket.selectedQuantity > 0).map(ticket => ({\n          name: ticket.name,\n          quantity: ticket.selectedQuantity\n        })) || [];\n      });\n    },\n    canCheckout() {\n      return this.paymentMethod && this.termsAccepted;\n    }\n  },\n  watch: {\n    currentStep() {\n      this.updateProgressLine();\n    },\n    getCart: {\n      handler() {\n        this.discountResult = null;\n        this.promoCode = null;\n      },\n      deep: true // important! watches nested ticket quantities\n    }\n  },\n\n  methods: {\n    ...mapMutations([\"setSelectedTickets\"]),\n    generateReference() {\n      let text = \"\";\n      const possible = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789\";\n      for (let i = 0; i < 10; i++) {\n        text += possible.charAt(Math.floor(Math.random() * possible.length));\n      }\n      return text;\n    },\n    extractSelectedTickets() {\n      const selected = this.getCart.flatMap(product => {\n        return product.event?.tickets?.filter(ticket => ticket.selectedQuantity > 0).map(ticket => ({\n          name: ticket.name,\n          quantity: ticket.selectedQuantity\n        })) || [];\n      });\n\n      // Save to Vuex\n      this.setSelectedTickets(selected);\n    },\n    async initializePayment() {\n      try {\n        this.spinner = true; // Show spinner while processing\n        const response = await axios.post(\"http://localhost:4000/api/initialize\", {\n          email: this.email,\n          amount: this.amount\n        });\n        console.log(response.data);\n        const {\n          authorization_url,\n          reference\n        } = response.data.data;\n        // Save reference for verification later (optional)\n        localStorage.setItem(\"paystack_reference\", reference);\n        localStorage.setItem(\"cartTotal\", this.amount);\n        this.spinner = false; // Hide spinner after processing\n\n        // Redirect to Paystack payment page\n        window.location.href = authorization_url;\n      } catch (error) {\n        alert(\"Payment initialization failed\");\n        console.error(error);\n      }\n    },\n    async getTicket() {\n      try {\n        this.reference = this.generateReference(); // generate and store\n        localStorage.setItem(\"paystack_reference\", this.reference);\n        this.spinner = true; // Show spinner while processing\n        const affiliate = localStorage.getItem(\"affiliateCode\");\n        const payload = {\n          reference: this.reference,\n          userId: this.getCart[0]?.user,\n          title: this.getCart[0]?.title,\n          contact: {\n            email: this.getContactInfo.email,\n            phone: this.getContactInfo.phone\n          },\n          tickets: this.getSelectedTickets,\n          price: this.cartTotal,\n          affiliate\n        };\n        console.log(\"Sending order info:\", payload);\n        const res = await axios.post(\"http://localhost:4000/api/order\", payload);\n        this.spinner = false; // Hide spinner after processing\n        alert(\"ticket has been sent to your email\");\n        //this.showQRCode = true;\n        this.$refs.myForm.reset();\n        this.$router.push({\n          name: \"home\"\n        });\n        console.log(\"Order info sent:\", res.data);\n      } catch (err) {\n        console.error(\"Failed to send order info:\", err);\n      }\n    },\n    async applyPromo() {\n      try {\n        const payload = {\n          code: this.promoCode,\n          orderTotal: this.cartTotal,\n          id: this.getCart[0].id\n        };\n        console.log(\"Payload being sent:\", payload);\n        const res = await axios.post(\"http://localhost:4000/api/apply-promo\", payload);\n        this.discountResult = res.data;\n        localStorage.setItem(\"discountedTotal\", res.data.newTotal);\n        localStorage.setItem(\"promoCode\", this.promoCode);\n        console.log(\"Promo response:\", res.data);\n      } catch (err) {\n        alert(`‚ùå ${err.response?.data?.message || err.message}`);\n      }\n    },\n    processPayment: () => {\n      window.alert(\"Payment recieved\");\n    },\n    close: () => {\n      console.log(\"You closed checkout page\");\n    },\n    updateProgressLine() {\n      const wrapper = this.$el.querySelector(\".stepper-wrapper\");\n      if (wrapper) {\n        const value = this.currentStep / (this.steps.length - 1);\n        wrapper.style.setProperty(\"--progress\", value);\n      }\n    },\n    nextStep() {\n      this.$store.dispatch(\"setContactInfo\", this.contact);\n      const selected = this.getCart.flatMap(product => product.event.tickets.filter(ticket => ticket.selectedQuantity > 0).map(ticket => ({\n        name: ticket.name,\n        quantity: ticket.selectedQuantity\n      })));\n\n      // Save to Vuex\n      this.$store.commit(\"setSelectedTickets\", selected);\n      if (this.currentStep < this.steps.length - 1) this.currentStep++;\n    },\n    prevStep() {\n      if (this.currentStep > 0) this.currentStep--;\n    },\n    checkout() {\n      alert(\"‚úÖ Checkout successful!\");\n    },\n    startTimer() {\n      let totalSeconds = 600;\n      this.countdown = setInterval(() => {\n        totalSeconds--;\n        const minutes = String(Math.floor(totalSeconds / 60)).padStart(2, \"0\");\n        const seconds = String(totalSeconds % 60).padStart(2, \"0\");\n        this.timer = `${minutes}:${seconds}`;\n        if (totalSeconds <= 0) {\n          clearInterval(this.countdown);\n          alert(\"‚è∞ Time expired! Please restart.\");\n          location.reload();\n        }\n      }, 1000);\n    }\n  },\n  mounted() {\n    this.startTimer();\n    this.updateProgressLine();\n  },\n  beforeUnmount() {\n    clearInterval(this.countdown);\n  }\n};","map":{"version":3,"names":["mapGetters","mapMutations","axios","paystack","QrcodeVue","spinner","ref","getChartByID","components","data","currentStep","timer","reference","showQRCode","countdown","full_name","publicKey","process","env","VUE_APP_PUBLIC_KEY","steps","tickets","promoCode","discountResult","contact","firstName","lastName","email","confirmEmail","phone","paymentMethod","termsAccepted","subscribe","computed","rawSubtotal","subtotal","getCart","reduce","sum","product","ticketTotal","event","innerSum","ticket","selectedQuantity","price","console","log","serviceCharge","charge","toFixed","subTotal","discountAmount","discount","discountType","discountValue","cartTotal","total","getContactInfo","amount","selectedTickets","flatMap","filter","map","name","quantity","canCheckout","watch","updateProgressLine","handler","deep","methods","generateReference","text","possible","i","charAt","Math","floor","random","length","extractSelectedTickets","selected","setSelectedTickets","initializePayment","response","post","authorization_url","localStorage","setItem","window","location","href","error","alert","getTicket","affiliate","getItem","payload","userId","user","title","getSelectedTickets","res","$refs","myForm","reset","$router","push","err","applyPromo","code","orderTotal","id","newTotal","message","processPayment","close","wrapper","$el","querySelector","value","style","setProperty","nextStep","$store","dispatch","commit","prevStep","checkout","startTimer","totalSeconds","setInterval","minutes","String","padStart","seconds","clearInterval","reload","mounted","beforeUnmount"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\event-ticket\\event-ticket-frontend\\src\\components\\Payment.vue"],"sourcesContent":["<template>\r\n  <div class=\"container my-5\">\r\n    <div class=\"ticket-footer d-block d-md-none p-2\">\r\n      <div class=\"text-white p-3 rounded-3 text-center\" style=\"background-color: #047143\">\r\n        <small>subTotal: {{ subTotal }}</small>\r\n        <div class=\"fw-bold fs-5 mb-1\">Total: ‚Ç¶{{ cartTotal.toLocaleString() }}</div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row\">\r\n      <!-- Step Progress Bar -->\r\n\r\n      <!-- Left column: steps -->\r\n\r\n      <div class=\"col-md-8\">\r\n        <!-- Progress Bar -->\r\n        <div class=\"stepper mb-5 position-relative\">\r\n          <!-- Progress Line -->\r\n          <div class=\"progress-line\">\r\n            <div\r\n              class=\"progress-fill\"\r\n              :style=\"{ width: (currentStep / (steps.length - 1)) * 100 + '%' }\"\r\n            ></div>\r\n          </div>\r\n\r\n          <!-- Steps -->\r\n          <div\r\n            v-for=\"(stepLabel, index) in steps\"\r\n            :key=\"index\"\r\n            class=\"currentStep text-center\"\r\n            :class=\"{\r\n              'step-active': currentStep === index,\r\n              'step-completed': index < currentStep,\r\n            }\"\r\n          >\r\n            <div class=\"circle\">\r\n              <span v-if=\"index < currentStep\">‚úî</span>\r\n              <span v-else>{{ index + 1 }}</span>\r\n            </div>\r\n            <div class=\"label\">{{ stepLabel }}</div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Step 1: Ticket Selection -->\r\n        <!-- Place this inside your Vue template -->\r\n        <div v-if=\"currentStep === 0\" class=\"ticket-section text-start\">\r\n          <h3>üéüÔ∏è Choose Tickets</h3>\r\n\r\n          <div class=\"ticket-card\" v-for=\"product in getCart\" :key=\"product._id\">\r\n            <!-- Loop through tickets inside each product -->\r\n            <div v-for=\"ticket in product.event.tickets\" :key=\"ticket._id\" class=\"\">\r\n              <div class=\"ticket-content d-flex justify-content-between\">\r\n                <div>\r\n                  <h5>{{ ticket.name }}</h5>\r\n                  <p class=\"price\">‚Ç¶{{ ticket.price || 0 }}</p>\r\n                </div>\r\n\r\n                <!-- Optional: Add a quantity selector if needed -->\r\n                <select\r\n                  class=\"ticket-select mt-2\"\r\n                  v-model.number=\"ticket.selectedQuantity\"\r\n                >\r\n                  <option v-for=\"n in ticket.quantity + 1\" :key=\"n\" :value=\"n - 1\">\r\n                    {{ n - 1 }}\r\n                  </option>\r\n                </select>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <small> Total includes a¬†service¬†fee </small>\r\n          <div class=\"d-block d-md-none\">\r\n            <input\r\n              v-model=\"promoCode\"\r\n              placeholder=\"Enter promo code\"\r\n              class=\"form-control mb-2\"\r\n            />\r\n\r\n            <button @click=\"applyPromo\" class=\"btn btn-primary\">Apply</button>\r\n\r\n            <div v-if=\"discountResult\">\r\n              <p>{{ discountResult.message }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Step 2: Contact Info -->\r\n        <div v-else-if=\"currentStep === 1\" class=\"text-start\">\r\n          <h3>üìá Contact Information</h3>\r\n\r\n          <form ref=\"myForm\" @submit.prevent=\"\">\r\n            <div class=\"row mb-3\">\r\n              <div class=\"col\">\r\n                <label class=\"form-label\">First Name *</label>\r\n                <input\r\n                  v-model=\"contact.firstName\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <label class=\"form-label\">Last Name *</label>\r\n                <input\r\n                  v-model=\"contact.lastName\"\r\n                  type=\"text\"\r\n                  class=\"form-control\"\r\n                  required\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">Email Address *</label>\r\n              <input v-model=\"contact.email\" type=\"email\" class=\"form-control\" required />\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">Confirm Email Address *</label>\r\n              <input\r\n                v-model=\"contact.confirmEmail\"\r\n                type=\"email\"\r\n                class=\"form-control\"\r\n                required\r\n              />\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">Phone Number *</label>\r\n              <div class=\"input-group\">\r\n                <span class=\"input-group-text\">+234</span>\r\n                <input v-model=\"contact.phone\" type=\"tel\" class=\"form-control\" required />\r\n              </div>\r\n            </div>\r\n          </form>\r\n        </div>\r\n\r\n        <!-- Step 3: Payment -->\r\n        <div v-else-if=\"currentStep === 2\" class=\"text-start\">\r\n          <h3>üí≥ Payment Options</h3>\r\n\r\n          <p class=\"alert alert-warning\">\r\n            We've reserved your ticket. Please complete checkout within\r\n            {{ timer }} to secure your tickets.\r\n          </p>\r\n\r\n          <div class=\"form-check mb-2\">\r\n            <input\r\n              class=\"form-check-input\"\r\n              type=\"radio\"\r\n              v-model=\"paymentMethod\"\r\n              value=\"card\"\r\n              id=\"payCard\"\r\n            />\r\n            <label class=\"form-check-label\" for=\"payCard\">Pay with Paystack</label>\r\n            <form>\r\n              <div class=\"mb-3\">\r\n                <label>Email</label>\r\n                <input\r\n                  :value=\"getContactInfo.email\"\r\n                  type=\"email\"\r\n                  class=\"form-control\"\r\n                  readonly\r\n                />\r\n              </div>\r\n            </form>\r\n          </div>\r\n\r\n          <div class=\"form-check mb-2\">\r\n            <input\r\n              class=\"form-check-input\"\r\n              type=\"checkbox\"\r\n              v-model=\"termsAccepted\"\r\n              id=\"terms\"\r\n            />\r\n            <label class=\"form-check-label\" for=\"terms\">\r\n              I accept the <a href=\"#\">234Africa Terms and Conditions</a>,\r\n              <a href=\"#\">Refund Policy</a> and <a href=\"#\">Privacy Policy</a>.\r\n            </label>\r\n          </div>\r\n        </div>\r\n        <div class=\"text-center mt-4 d-flex justify-content-between\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn btn-danger\"\r\n            @click=\"prevStep\"\r\n            :disabled=\"currentStep === 0\"\r\n          >\r\n            Back\r\n          </button>\r\n          <button\r\n            v-if=\"currentStep === 2\"\r\n            type=\"submit\"\r\n            @click=\"initializePayment()\"\r\n            :disabled=\"!termsAccepted\"\r\n            class=\"btn btn-success\"\r\n          >\r\n            Pay Now ‚Ç¶{{ cartTotal.toLocaleString() }}\r\n          </button>\r\n          <button\r\n            v-if=\"currentStep < steps.length - 1\"\r\n            type=\"button\"\r\n            class=\"btn btn-primary\"\r\n            @click=\"nextStep\"\r\n          >\r\n            Next\r\n          </button>\r\n\r\n          <!-- Only show when cartTotal is 0 -->\r\n          <button\r\n            v-else-if=\"cartTotal === 0\"\r\n            type=\"submit\"\r\n            class=\"btn btn-primary\"\r\n            @click=\"getTicket()\"\r\n          >\r\n            Get tickets\r\n          </button>\r\n        </div>\r\n        <spinner v-if=\"spinner\" />\r\n      </div>\r\n\r\n      <!-- Right column: Summary -->\r\n      <div class=\"col-md-4 text-start d-none d-md-block\">\r\n        <div class=\"p-4 bg-light border-start\">\r\n          <h5>Summary</h5>\r\n          <div v-if=\"selectedTickets.length\" class=\"mt-4\">\r\n            <h5>üéüÔ∏è Selected Tickets:</h5>\r\n            <ul>\r\n              <li v-for=\"(ticket, index) in selectedTickets\" :key=\"index\">\r\n                {{ ticket.name }} x {{ ticket.quantity }}\r\n              </li>\r\n            </ul>\r\n          </div>\r\n\r\n          <small>subTotal: {{ subTotal }}</small>\r\n          <div class=\"fw-semibold fs-6\">Service Charge (7.5%): ‚Ç¶{{ serviceCharge }}</div>\r\n\r\n          <div class=\"fw-bold fs-5\">Total: ‚Ç¶{{ cartTotal }}</div>\r\n        </div>\r\n        <div class=\"\">\r\n          <h2>Apply Promo Code</h2>\r\n          <input\r\n            v-model=\"promoCode\"\r\n            placeholder=\"Enter promo code\"\r\n            class=\"form-control mb-2\"\r\n          />\r\n\r\n          <button @click=\"applyPromo\" class=\"btn btn-primary\">Apply</button>\r\n\r\n          <div v-if=\"discountResult\">\r\n            <p>{{ discountResult.message }}</p>\r\n            <p>New Total: ‚Ç¶{{ discountResult.newTotal }}</p>\r\n            <p>Discount: ‚Ç¶{{ discountResult.discount }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapMutations } from \"vuex\";\r\nimport axios from \"axios\";\r\nimport paystack from \"vue-paystack\";\r\nimport QrcodeVue from \"qrcode.vue\";\r\nimport spinner from \"./spinner.vue\";\r\nimport { ref } from \"vue\";\r\nimport { getChartByID } from \"apexcharts\";\r\nexport default {\r\n  components: {\r\n    paystack,\r\n    QrcodeVue,\r\n    spinner,\r\n  },\r\n  data() {\r\n    return {\r\n      currentStep: 0,\r\n      timer: \"10:00\",\r\n      spinner: false,\r\n      reference: \"\",\r\n      showQRCode: false,\r\n      countdown: null,\r\n      full_name: \"\",\r\n      publicKey: process.env.VUE_APP_PUBLIC_KEY,\r\n      steps: [\"Tickets\", \"Contact\", \"Payment\"],\r\n      tickets: [],\r\n      promoCode: \"\",\r\n      discountResult: null,\r\n      contact: {\r\n        firstName: \"\",\r\n        lastName: \"\",\r\n        email: \"\",\r\n        confirmEmail: \"\",\r\n        phone: \"\",\r\n      },\r\n      paymentMethod: \"\",\r\n      termsAccepted: false,\r\n      subscribe: false,\r\n    };\r\n  },\r\n  computed: {\r\n    // 1. Raw subtotal (tickets only, no service, no discount)\r\n    rawSubtotal() {\r\n      const subtotal = this.getCart.reduce((sum, product) => {\r\n        const ticketTotal = product.event.tickets.reduce((innerSum, ticket) => {\r\n          return innerSum + (ticket.selectedQuantity || 0) * (ticket.price || 0);\r\n        }, 0);\r\n        return sum + ticketTotal;\r\n      }, 0);\r\n\r\n      console.log(\"Raw Subtotal:\", subtotal);\r\n      return subtotal;\r\n    },\r\n\r\n    // 2. Service charge (based on raw subtotal)\r\n    serviceCharge() {\r\n      const charge = this.rawSubtotal * 0.075;\r\n      console.log(\"Service Charge (7.5%):\", charge);\r\n      return +charge.toFixed(2);\r\n    },\r\n\r\n    // 3. Subtotal before discount (subtotal + service charge)\r\n    subTotal() {\r\n      const subtotal = this.rawSubtotal + this.serviceCharge;\r\n      console.log(\"Subtotal Before Discount:\", subtotal);\r\n      return +subtotal.toFixed(2);\r\n    },\r\n\r\n    // 4. Discount amount (applied on subtotal before discount)\r\n    discountAmount() {\r\n      if (!this.discountResult) return 0;\r\n\r\n      let discount = 0;\r\n      if (this.discountResult.discountType === \"percentage\") {\r\n        discount = this.subTotal * (this.discountResult.discountValue / 100);\r\n      } else if (this.discountResult.discountType === \"flat\") {\r\n        discount = this.discountResult.discountValue;\r\n      }\r\n\r\n      console.log(\"Discount Applied:\", discount);\r\n      return +discount.toFixed(2);\r\n    },\r\n\r\n    // 5. Final total (subtotal - discount)\r\n    cartTotal() {\r\n      const total = this.subTotal - this.discountAmount;\r\n      console.log(\"Final Cart Total:\", total);\r\n      return +total.toFixed(2);\r\n    },\r\n\r\n    email() {\r\n      return this.getContactInfo.email;\r\n    },\r\n    amount() {\r\n      return this.cartTotal;\r\n    },\r\n    steps() {\r\n      // If the cart has a total greater than 0, show payment step\r\n      return this.cartTotal > 0\r\n        ? [\"Tickets\", \"Contact\", \"Payment\"]\r\n        : [\"Tickets\", \"Contact\"];\r\n    },\r\n    ...mapGetters([\"getCart\", \"getContactInfo\", \"getSelectedTickets\", \"getCartTotal\"]),\r\n\r\n    selectedTickets() {\r\n      return this.getCart.flatMap((product) => {\r\n        return (\r\n          product.event?.tickets\r\n            ?.filter((ticket) => ticket.selectedQuantity > 0)\r\n            .map((ticket) => ({\r\n              name: ticket.name,\r\n              quantity: ticket.selectedQuantity,\r\n            })) || []\r\n        );\r\n      });\r\n    },\r\n\r\n    canCheckout() {\r\n      return this.paymentMethod && this.termsAccepted;\r\n    },\r\n  },\r\n  watch: {\r\n    currentStep() {\r\n      this.updateProgressLine();\r\n    },\r\n    getCart: {\r\n      handler() {\r\n        this.discountResult = null;\r\n        this.promoCode = null;\r\n      },\r\n      deep: true, // important! watches nested ticket quantities\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"setSelectedTickets\"]),\r\n    generateReference() {\r\n      let text = \"\";\r\n      const possible = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789\";\r\n      for (let i = 0; i < 10; i++) {\r\n        text += possible.charAt(Math.floor(Math.random() * possible.length));\r\n      }\r\n      return text;\r\n    },\r\n    extractSelectedTickets() {\r\n      const selected = this.getCart.flatMap((product) => {\r\n        return (\r\n          product.event?.tickets\r\n            ?.filter((ticket) => ticket.selectedQuantity > 0)\r\n            .map((ticket) => ({\r\n              name: ticket.name,\r\n              quantity: ticket.selectedQuantity,\r\n            })) || []\r\n        );\r\n      });\r\n\r\n      // Save to Vuex\r\n      this.setSelectedTickets(selected);\r\n    },\r\n    async initializePayment() {\r\n      try {\r\n        this.spinner = true; // Show spinner while processing\r\n        const response = await axios.post(\"http://localhost:4000/api/initialize\", {\r\n          email: this.email,\r\n          amount: this.amount,\r\n        });\r\n        console.log(response.data);\r\n\r\n        const { authorization_url, reference } = response.data.data;\r\n        // Save reference for verification later (optional)\r\n        localStorage.setItem(\"paystack_reference\", reference);\r\n        localStorage.setItem(\"cartTotal\", this.amount);\r\n        this.spinner = false; // Hide spinner after processing\r\n\r\n        // Redirect to Paystack payment page\r\n        window.location.href = authorization_url;\r\n    