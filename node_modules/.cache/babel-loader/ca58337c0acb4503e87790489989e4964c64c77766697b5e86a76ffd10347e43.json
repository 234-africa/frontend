{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nimport { QuillEditor } from \"@vueup/vue-quill\";\nimport \"@vueup/vue-quill/dist/vue-quill.snow.css\";\nimport spinner from \"../spinner.vue\";\nexport default {\n  components: {\n    QuillEditor,\n    spinner\n  },\n  data() {\n    return {\n      //  customizeUrl: \"\",\n      urlStatus: \"\",\n      statusClass: \"\",\n      currentStep: 0,\n      steps: [\"Basic Info\", \"Event Details\", \"Media Upload\", \"Tickets\"],\n      categories: [],\n      spinner: false,\n      categoryID: \"\",\n      product: {\n        title: \"\",\n        description: \"\",\n        currency: \"\",\n        customizeUrl: \"\",\n        tag: \"\",\n        price: \"\",\n        event: {\n          startDate: \"\",\n          endDate: \"\",\n          startTime: \"\",\n          timezone: \"UTC\",\n          location: \"\"\n        }\n      },\n      address: \"\",\n      eventType: \"single\",\n      // or \"recurring\"\n      eventFrequency: \"\",\n      photos: [],\n      previewUrl: null,\n      tickets: [{\n        name: \"\",\n        price: null,\n        quantity: null\n      }]\n    };\n  },\n  computed: {\n    ...mapGetters([\"getToken\"])\n  },\n  async created() {\n    try {\n      const res = await axios.get(\"https://event-ticket-backend-gnmw.onrender.com/api/categories\");\n      this.categories = res.data.categories || res.data;\n      console.log(\"Categories fetched:\", this.categories);\n    } catch (error) {\n      console.error(\"Failed to fetch categories:\", error);\n    }\n  },\n  methods: {\n    debouncedCheck() {\n      clearTimeout(this.debounceTimer);\n      this.debounceTimer = setTimeout(() => {\n        this.checkCustomUrl();\n      }, 500); // Wait 500ms after typing stops\n    },\n    async checkUrl() {\n      if (!this.product.customizeUrl.trim()) {\n        this.urlStatus = \"\";\n        return;\n      }\n      try {\n        const res = await axios.post(\"https://event-ticket-backend-gnmw.onrender.com/api/check-custom-url\", {\n          url: this.product.customizeUrl\n        });\n        if (res.data.success) {\n          this.urlStatus = res.data.message; // \"This URL is available\"\n          this.statusClass = \"text-success\";\n        } else {\n          this.urlStatus = res.data.message; // \"This URL is already taken\"\n          this.statusClass = \"text-danger\";\n        }\n      } catch (err) {\n        this.urlStatus = \"Error checking URL\";\n        this.statusClass = \"text-danger\";\n      }\n    },\n    isValidTitle() {\n      return this.product.title.trim().split(/\\s+/).length >= 2;\n    },\n    nextStep() {\n      if (this.currentStep < this.steps.length - 1) {\n        this.currentStep++;\n      }\n    },\n    prevStep() {\n      if (this.currentStep > 0) {\n        this.currentStep--;\n      }\n    },\n    handleFiles(event) {\n      const files = Array.from(event.target.files);\n      this.photos = files;\n      if (files.length > 0) {\n        const reader = new FileReader();\n        reader.onload = e => {\n          this.previewUrl = e.target.result;\n        };\n        reader.readAsDataURL(files[0]);\n      }\n    },\n    addTicket(type) {\n      const defaultPrice = type === \"Paid\" ? \"Cost\" : type === \"Donation\" ? \"Any\" : \"Free\";\n      this.tickets.push({\n        name: \"\",\n        quantity: null,\n        price: null\n      });\n    },\n    removeTicket(index) {\n      this.tickets.splice(index, 1);\n    },\n    duplicateTicket(index) {\n      const ticket = this.tickets[index];\n      this.tickets.splice(index + 1, 0, {\n        ...ticket\n      });\n    },\n    async submitProduct() {\n      this.spinner = true;\n      const formData = new FormData();\n      formData.append(\"categoryID\", this.categoryID);\n      formData.append(\"title\", this.product.title);\n      formData.append(\"description\", this.product.description);\n      formData.append(\"tag\", this.product.tag);\n      formData.append(\"customizeUrl\", this.product.customizeUrl);\n      formData.append(\"price\", this.product.price);\n      formData.append(\"currency\", this.product.currency);\n      formData.append(\"eventDate\", new Date(this.product.event.startDate).toISOString());\n      if (this.product.event.endDate) {\n        formData.append(\"endDate\", new Date(this.product.event.endDate).toISOString());\n      }\n      formData.append(\"startTime\", this.product.event.startTime + \" \");\n      formData.append(\"timezone\", this.product.event.timezone);\n      formData.append(\"locationName\", this.address); // Send address as name\n\n      if (this.eventType === \"recurring\") {\n        formData.append(\"eventFrequency\", this.eventFrequency);\n      }\n      formData.append(\"tickets\", JSON.stringify(this.tickets));\n      console.log(this.tickets);\n      this.photos.forEach(photo => {\n        formData.append(\"photos\", photo);\n      });\n      for (let [key, value] of formData.entries()) {\n        console.log(`${key}:`, value);\n      }\n      try {\n        const response = await axios.post(\"https://event-ticket-backend-gnmw.onrender.com/api/products\", formData, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`,\n            \"Content-Type\": \"multipart/form-data\"\n          }\n        });\n        console.log(\"Product created:\", response.data);\n        alert(\"Event created successfully!\");\n      } catch (error) {\n        if (error.response && error.response.status === 401) {\n          localStorage.removeItem(\"token\");\n          router.push(\"/login\");\n        } else {\n          throw error; // Other errors get thrown normally\n        }\n      }\n      this.spinner = false;\n    }\n  }\n};","map":{"version":3,"names":["axios","mapGetters","QuillEditor","spinner","components","data","urlStatus","statusClass","currentStep","steps","categories","categoryID","product","title","description","currency","customizeUrl","tag","price","event","startDate","endDate","startTime","timezone","location","address","eventType","eventFrequency","photos","previewUrl","tickets","name","quantity","computed","created","res","get","console","log","error","methods","debouncedCheck","clearTimeout","debounceTimer","setTimeout","checkCustomUrl","checkUrl","trim","post","url","success","message","err","isValidTitle","split","length","nextStep","prevStep","handleFiles","files","Array","from","target","reader","FileReader","onload","e","result","readAsDataURL","addTicket","type","defaultPrice","push","removeTicket","index","splice","duplicateTicket","ticket","submitProduct","formData","FormData","append","Date","toISOString","JSON","stringify","forEach","photo","key","value","entries","response","headers","Authorization","getToken","alert","status","localStorage","removeItem","router"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\xASn2901-Daredevil\\event-ticket-frontend\\src\\components\\app\\addEvent.vue"],"sourcesContent":["<template>\r\n  <div class=\" \">\r\n    <!-- Stepper -->\r\n    <div class=\"stepper pt-2 mx-auto\">\r\n      <div\r\n        v-for=\"(step, index) in steps\"\r\n        :key=\"index\"\r\n        class=\"step\"\r\n        :class=\"{\r\n          'step-active': currentStep === index,\r\n          completed: index < currentStep,\r\n        }\"\r\n      >\r\n        <div class=\"circle\">\r\n          <span v-if=\"index < currentStep\">âœ”</span>\r\n          <span v-else>{{ index + 1 }}</span>\r\n        </div>\r\n        <div class=\"label\">{{ step }}</div>\r\n      </div>\r\n      <div class=\"progress-line\">\r\n        <div\r\n          class=\"progress-fill\"\r\n          :style=\"{ width: (currentStep / (steps.length - 1)) * 100 + '%' }\"\r\n        ></div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Form -->\r\n    <div class=\"row justify-content-center\">\r\n      <div class=\"col-12\">\r\n        <form\r\n          @submit.prevent=\"submitProduct\"\r\n          enctype=\"multipart/form-data\"\r\n          class=\"shadow p-4 w-md-75 mx-auto w-100\"\r\n        >\r\n          <!-- STEP 1: Basic Info -->\r\n          <div class=\"\" v-if=\"currentStep === 0\">\r\n            <div class=\"row\">\r\n              <div class=\"mb-3 col-md-7\">\r\n                <label for=\"title\" class=\"form-label\">What is your event name:</label>\r\n                <input\r\n                  v-model=\"product.title\"\r\n                  placeholder=\"Tech Summit, Afro summit\"\r\n                  type=\"text\"\r\n                  id=\"title\"\r\n                  :class=\"{ 'is-invalid': product.title && !isValidTitle() }\"\r\n                  class=\"form-control\"\r\n                />\r\n                <div v-if=\"product.title && !isValidTitle()\" class=\"invalid-feedback\">\r\n                  Title must be at least two words.\r\n                </div>\r\n              </div>\r\n              <div class=\"mb-3 col-md-5\">\r\n                <div>\r\n                  <div>\r\n                    <label for=\"title\" class=\"form-label\">Tag:</label>\r\n                    <input\r\n                      v-model=\"product.tag\"\r\n                      type=\"text\"\r\n                      placeholder=\"Tags (comma-separated)\"\r\n                      id=\"title\"\r\n                      class=\"form-control\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"row mb-3\">\r\n              <div class=\"col-md-7\">\r\n                <label class=\"form-label\">Event Category</label>\r\n                <div>\r\n                  <select\r\n                    class=\"form-select\"\r\n                    v-if=\"categories.length > 0\"\r\n                    v-model=\"categoryID\"\r\n                  >\r\n                    <option value=\"\" disabled>Select a category</option>\r\n                    <option\r\n                      v-for=\"category in categories\"\r\n                      :key=\"category._id\"\r\n                      :value=\"category._id\"\r\n                    >\r\n                      {{ category.type }}\r\n                    </option>\r\n                    <!-- Add more as needed -->\r\n                  </select>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-5\">\r\n                <div>\r\n                  <label for=\"customUrl\" class=\"form-label\">Use custom URL</label>\r\n                  <div class=\"input-group pt-2\">\r\n                    <span class=\"input-group-text\">234Africa.live/event/</span>\r\n                    <input\r\n                      type=\"text\"\r\n                      v-model=\"product.customizeUrl\"\r\n                      @input=\"checkUrl\"\r\n                      class=\"form-control pt-md-2\"\r\n                      id=\"customUrl\"\r\n                      placeholder=\"Enter your custom link\"\r\n                    />\r\n                  </div>\r\n                  <small v-if=\"urlStatus\" :class=\"statusClass\">{{ urlStatus }}</small>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label\">What's your event about:</label>\r\n              <div>\r\n                <textarea class=\"w-100 h-50\" v-model=\"product.description\" />\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- STEP 2: Description -->\r\n          <div v-if=\"currentStep === 1\">\r\n            <div class=\"mt-4 text-start\">\r\n              <h4><strong>When is your event?</strong></h4>\r\n              <p class=\"text-muted\">Select all the dates of your event</p>\r\n\r\n              <!-- Time Zone -->\r\n              <div class=\"row mb-3\">\r\n                <div class=\"col-md-6\">\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"address\" class=\"form-label\">Enter Address:</label>\r\n                    <input\r\n                      v-model=\"address\"\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      id=\"address\"\r\n                      placeholder=\"e.g. 1600 Amphitheatre Parkway, CA\"\r\n                      required\r\n                    />\r\n                  </div>\r\n                </div>\r\n\r\n                <div class=\"col-md-6\">\r\n                  <label class=\"form-label\">* Select time zone</label>\r\n                  <select class=\"form-select\" v-model=\"product.event.timezone\">\r\n                    <option value=\"\">Select Time Zone</option>\r\n                    <option value=\"UTC+01:00\">(UTC+01:00) West Central Africa</option>\r\n                    <option value=\"UTC+00:00\">(UTC+00:00) GMT</option>\r\n                    <option value=\"UTC-05:00\">\r\n                      (UTC-05:00) Eastern Time (US & Canada)\r\n                    </option>\r\n                    <!-- Add more as needed -->\r\n                  </select>\r\n                </div>\r\n              </div>\r\n              <!-- Toggle Buttons -->\r\n              <div class=\"btn-group mb-3\" role=\"group\">\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn\"\r\n                  :class=\"\r\n                    eventType === 'single'\r\n                      ? 'btn-primary active'\r\n                      : 'btn-outline-secondary'\r\n                  \"\r\n                  @click=\"eventType = 'single'\"\r\n                >\r\n                  Single Event\r\n                </button>\r\n                <button\r\n                  type=\"button\"\r\n                  class=\"btn\"\r\n                  :class=\"\r\n                    eventType === 'recurring'\r\n                      ? 'btn-primary active'\r\n                      : 'btn-outline-secondary'\r\n                  \"\r\n                  @click=\"eventType = 'recurring'\"\r\n                >\r\n                  Recurring Event\r\n                </button>\r\n              </div>\r\n              <!-- Start/End Date and Time -->\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6 mb-3\">\r\n                  <label class=\"form-label\">* Start date</label>\r\n                  <input\r\n                    type=\"date\"\r\n                    class=\"form-control\"\r\n                    v-model=\"product.event.startDate\"\r\n                  />\r\n                </div>\r\n                <div class=\"col-md-6 mb-3\" v-if=\"eventType === 'recurring'\">\r\n                  <label class=\"form-label\">* End date</label>\r\n                  <input\r\n                    type=\"date\"\r\n                    class=\"form-control\"\r\n                    v-model=\"product.event.endDate\"\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6 mb-3\">\r\n                  <label class=\"form-label\">Start time</label>\r\n                  <div class=\"d-flex justify-content-between\">\r\n                    <div class=\"mt-2\">\r\n                      <input\r\n                        type=\"time\"\r\n                        class=\"form-control\"\r\n                        v-model=\"product.event.startTime\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n\r\n                <!-- Recurring Options -->\r\n                <div class=\"col-md-4 mb-3\" v-if=\"eventType === 'recurring'\">\r\n                  <label class=\"form-label\">* Event frequency</label>\r\n                  <select class=\"form-select\" v-model=\"eventFrequency\">\r\n                    <option>Every day</option>\r\n                    <option>Weekdays</option>\r\n                    <option>Weekends</option>\r\n                    <option>Every week</option>\r\n                  </select>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- STEP 3: Media Upload -->\r\n          <div v-if=\"currentStep === 2\">\r\n            <div class=\"container py-5 text-start\">\r\n              <div class=\"row\">\r\n                <!-- Left Column: Event Image -->\r\n                <div class=\"col-md-6\">\r\n                  <h5><strong>Event Image</strong></h5>\r\n                  <p class=\"text-muted\">Upload a JPEG or PNG file</p>\r\n\r\n                  <div\r\n                    class=\"alert alert-light border text-muted py-2 small d-flex align-items-start\"\r\n                  >\r\n                    <i class=\"bi bi-info-circle-fill me-2 text-danger\"></i>\r\n                    Images with a 1:1 ratio (a square) work best on all event themes\r\n                  </div>\r\n\r\n                  <div>\r\n                    <!-- Upload box -->\r\n                    <label\r\n                      for=\"imageUpload\"\r\n                      class=\"border rounded bg-light d-flex flex-column justify-content-center align-items-center text-center py-5 mb-2\"\r\n                      style=\"cursor: pointer\"\r\n                    >\r\n                      <i class=\"bi bi-image fs-1 mb-2\"></i>\r\n                      <span>\r\n                        Drag an image here or\r\n                        <span class=\"text-danger fw-bold\">click to upload</span>\r\n                      </span>\r\n                      <input\r\n                        type=\"file\"\r\n                        multiple\r\n                        @change=\"handleFiles\"\r\n                        accept=\"image/*\"\r\n                        id=\"imageUpload\"\r\n                        class=\"d-none\"\r\n                      />\r\n                      <p v-if=\"photos.length\">Selected: {{ photos.length }} image(s)</p>\r\n                    </label>\r\n\r\n                    <!-- Preview -->\r\n                    <div v-if=\"previewUrl\" class=\"text-center mt-3\">\r\n                      <img\r\n                        :src=\"previewUrl\"\r\n                        alt=\"Preview\"\r\n                        class=\"img-fluid rounded shadow\"\r\n                        style=\"max-height: 250px\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                  <p class=\"text-muted small\">\r\n                    Upload an image with a size less than 2mb\r\n                  </p>\r\n                </div>\r\n\r\n                <!-- Right Column: Event Theme -->\r\n                <div class=\"col-md-6\"></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- STEP 4: Location -->\r\n          <div v-if=\"currentStep === 3\">\r\n            <div class=\"my-4 text-start\">\r\n              <h5><strong>Create your ticket types</strong></h5>\r\n              <p class=\"text-danger small\"></p>\r\n\r\n              <!-- Ticket Rows -->\r\n              <div\r\n                v-for=\"(ticket, index) in tickets\"\r\n                :key=\"index\"\r\n                class=\"row g-3 align-items-end mb-2\"\r\n              >\r\n                <div class=\"col-md-3\">\r\n                  <label class=\"form-label\">Ticket name</label>\r\n                  <input\r\n                    v-model=\"ticket.name\"\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"e.g. General Admission\"\r\n                  />\r\n                </div>\r\n\r\n                <div class=\"col-md-2\">\r\n                  <label class=\"form-label\">Quantity</label>\r\n                  <input\r\n                    v-model.number=\"ticket.quantity\"\r\n                    type=\"number\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n\r\n                <div class=\"col-md-3\">\r\n                  <label class=\"form-label\">Price</label>\r\n                  <input\r\n                    v-model.number=\"ticket.price\"\r\n                    type=\"number\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"Blank for free event\"\r\n                  />\r\n                </div>\r\n\r\n                <div class=\"col-md-2 d-flex align-items-end\">\r\n                  <button\r\n                    class=\"btn btn-outline-danger w-25\"\r\n                    title=\"Delete\"\r\n                    @click=\"removeTicket(index)\"\r\n                  >\r\n                    <i class=\"bi bi-trash\"></i>\r\n                  </button>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- Timeslot Capacity -->\r\n\r\n              <!-- Add Ticket Buttons -->\r\n              <div class=\"d-flex gap-3\">\r\n                <button\r\n                  class=\"btn btn-primary btn-primary:hover\"\r\n                  type=\"button\"\r\n                  @click=\"addTicket('Paid')\"\r\n                >\r\n                  + ticket\r\n                </button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Buttons -->\r\n          <div class=\"text-center mt-4 d-flex justify-content-between\">\r\n            <button\r\n              type=\"button\"\r\n              class=\"btn btn-danger\"\r\n              @click=\"prevStep\"\r\n              :disabled=\"currentStep === 0\"\r\n            >\r\n              Back\r\n            </button>\r\n            <button\r\n              v-if=\"currentStep < steps.length - 1\"\r\n              type=\"button\"\r\n              class=\"btn btn-primary btn-primary:hover\"\r\n              @click=\"nextStep\"\r\n            >\r\n              Next\r\n            </button>\r\n            <button v-else type=\"submit\" class=\"btn btn-primary btn-primary:hover\">\r\n              Create Event\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n    <spinner v-if=\"spinner\" />\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\nimport { QuillEditor } from \"@vueup/vue-quill\";\r\nimport \"@vueup/vue-quill/dist/vue-quill.snow.css\";\r\nimport spinner from \"../spinner.vue\";\r\nexport default {\r\n  components: {\r\n    QuillEditor,\r\n    spinner,\r\n  },\r\n  data() {\r\n    return {\r\n      //  customizeUrl: \"\",\r\n      urlStatus: \"\",\r\n      statusClass: \"\",\r\n      currentStep: 0,\r\n      steps: [\"Basic Info\", \"Event Details\", \"Media Upload\", \"Tickets\"],\r\n      categories: [],\r\n      spinner: false,\r\n      categoryID: \"\",\r\n      product: {\r\n        title: \"\",\r\n        description: \"\",\r\n        currency: \"\",\r\n        customizeUrl: \"\",\r\n        tag: \"\",\r\n        price: \"\",\r\n        event: {\r\n          startDate: \"\",\r\n          endDate: \"\",\r\n          startTime: \"\",\r\n\r\n          timezone: \"UTC\",\r\n          location: \"\",\r\n        },\r\n      },\r\n      address: \"\",\r\n\r\n      eventType: \"single\", // or \"recurring\"\r\n      eventFrequency: \"\",\r\n      photos: [],\r\n      previewUrl: null,\r\n      tickets: [{ name: \"\", price: null, quantity: null }],\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n  async created() {\r\n    try {\r\n      const res = await axios.get(\"https://event-ticket-backend-gnmw.onrender.com/api/categories\");\r\n      this.categories = res.data.categories || res.data;\r\n      console.log(\"Categories fetched:\", this.categories);\r\n    } catch (error) {\r\n      console.error(\"Failed to fetch categories:\", error);\r\n    }\r\n  },\r\n  methods: {\r\n    debouncedCheck() {\r\n      clearTimeout(this.debounceTimer);\r\n      this.debounceTimer = setTimeout(() => {\r\n        this.checkCustomUrl();\r\n      }, 500); // Wait 500ms after typing stops\r\n    },\r\n    async checkUrl() {\r\n      if (!this.product.customizeUrl.trim()) {\r\n        this.urlStatus = \"\";\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const res = await axios.post(\"https://event-ticket-backend-gnmw.onrender.com/api/check-custom-url\", {\r\n          url: this.product.customizeUrl,\r\n        });\r\n\r\n        if (res.data.success) {\r\n          this.urlStatus = res.data.message; // \"This URL is available\"\r\n          this.statusClass = \"text-success\";\r\n        } else {\r\n          this.urlStatus = res.data.message; // \"This URL is already taken\"\r\n          this.statusClass = \"text-danger\";\r\n        }\r\n      } catch (err) {\r\n        this.urlStatus = \"Error checking URL\";\r\n        this.statusClass = \"text-danger\";\r\n      }\r\n    },\r\n    isValidTitle() {\r\n      return this.product.title.trim().split(/\\s+/).length >= 2;\r\n    },\r\n\r\n    nextStep() {\r\n      if (this.currentStep < this.steps.length - 1) {\r\n        this.currentStep++;\r\n      }\r\n    },\r\n    prevStep() {\r\n      if (this.currentStep > 0) {\r\n        this.currentStep--;\r\n      }\r\n    },\r\n    handleFiles(event) {\r\n      const files = Array.from(event.target.files);\r\n      this.photos = files;\r\n      if (files.length > 0) {\r\n        const reader = new FileReader();\r\n        reader.onload = (e) => {\r\n          this.previewUrl = e.target.result;\r\n        };\r\n        reader.readAsDataURL(files[0]);\r\n      }\r\n    },\r\n    addTicket(type) {\r\n      const defaultPrice =\r\n        type === \"Paid\" ? \"Cost\" : type === \"Donation\" ? \"Any\" : \"Free\";\r\n      this.tickets.push({\r\n        name: \"\",\r\n        quantity: null,\r\n