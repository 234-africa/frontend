} catch (err) {\r\n        console.error(\"Error fetching banks:\", err);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card {\r\n  border-radius: 6px;\r\n  overflow: hidden;\r\n}\r\n\r\n.table-responsive {\r\n  overflow-x: hidden;\r\n}\r\n\r\n.custom-table {\r\n  width: 100%;\r\n  border-collapse: separate;\r\n  border-spacing: 0;\r\n}\r\n\r\n.custom-table thead {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.custom-table th,\r\n.custom-table td {\r\n  padding: 12px;\r\n  vertical-align: middle;\r\n}\r\n\r\n.custom-table tr:nth-child(even) {\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.custom-table tr:hover {\r\n  background-color: #f1f1f1;\r\n}\r\n</style>\r\n"],"mappings":";;AA6CA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGL,UAAU,CAAC,CAAC,UAAU,CAAC,CAAC;IAC3BM,aAAaA,CAAA,EAAG;MACd,MAAMC,KAAI,GAAI,IAAI,CAACH,WAAW,CAACI,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;MACnD,IAAI,CAACF,KAAK,EAAE,OAAO,IAAI,CAACJ,KAAK;MAE7B,OAAO,IAAI,CAACA,KAAK,CAACO,MAAM,CAAEC,IAAI,IAAK;QACjC,OACEA,IAAI,CAACC,WAAW,EAAEH,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACN,KAAK,KAC9CI,IAAI,CAACG,aAAa,EAAEL,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACN,KAAK,KAChDI,IAAI,CAACI,QAAQ,EAAEN,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACN,KAAK,KAC3CI,IAAI,CAACK,IAAI,EAAEP,WAAW,CAAC,CAAC,CAACI,QAAQ,CAACN,KAAK;MAE3C,CAAC,CAAC;IACJ;EACF,CAAC;EACDU,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,GAAE,GAAI,MAAMrB,KAAK,CAACsB,GAAG,CAAC,8DAA8D,CAAC;QAC3F,IAAI,CAAClB,KAAI,GAAIiB,GAAG,CAAClB,IAAI,CAACC,KAAK;MAC7B,EAAE,OAAOmB,GAAG,EAAE;QACZC,OAAO,CAACC,KAAK,CAAC,uBAAuB,EAAEF,GAAG,CAAC;MAC7C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}