{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from \"axios\";\nimport { mapGetters } from \"vuex\";\nexport default {\n  name: \"BankManager\",\n  data() {\n    return {\n      form: {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\"\n      },\n      banks: [],\n      isEditing: false,\n      editId: null\n    };\n  },\n  mounted() {\n    this.fetchBanks();\n  },\n  computed: {\n    ...mapGetters([\"getToken\"])\n  },\n  methods: {\n    /** ‚úÖ BANK METHODS **/\n    async fetchBanks() {\n      try {\n        const res = await axios.get(\"https://event-ticket-backend-gnmw.onrender.com/api/bank\", {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        this.banks = res.data.banks;\n      } catch (err) {\n        console.error(\"Error fetching banks:\", err);\n      }\n    },\n    async createBank() {\n      try {\n        const res = await axios.post(\"https://event-ticket-backend-gnmw.onrender.com/api/bank\", this.form, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        if (res.data.banks && res.data.banks.length > 0) {\n          this.banks.push(res.data.banks[0]);\n        } else {\n          await this.fetchBanks();\n        }\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error creating bank:\", err);\n        alert(\"‚ùå Failed to create.\");\n      }\n    },\n    editBank(bank) {\n      this.isEditing = true;\n      this.editId = bank._id;\n      this.form = {\n        ...bank\n      };\n    },\n    cancelEdit() {\n      this.resetForm();\n    },\n    async updateBank() {\n      try {\n        const res = await axios.put(`https://event-ticket-backend-gnmw.onrender.com/api/bank/${this.editId}`, this.form, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        const index = this.banks.findIndex(b => b._id === this.editId);\n        if (res.data.banks && res.data.banks.length > 0) {\n          this.banks.splice(index, 1, res.data.banks[0]);\n        } else {\n          await this.fetchBanks();\n        }\n        this.resetForm();\n      } catch (err) {\n        console.error(\"Error updating bank:\", err);\n      }\n    },\n    async deleteBank(id) {\n      if (!confirm(\"Delete this bank info?\")) return;\n      try {\n        await axios.delete(`https://event-ticket-backend-gnmw.onrender.com/api/bank/${id}`, {\n          headers: {\n            Authorization: `Bearer ${this.getToken}`\n          }\n        });\n        this.banks = this.banks.filter(b => b._id !== id);\n      } catch (err) {\n        console.error(\"Error deleting bank:\", err);\n      }\n    },\n    resetForm() {\n      this.form = {\n        accountName: \"\",\n        accountNumber: \"\",\n        bankName: \"\"\n      };\n      this.isEditing = false;\n      this.editId = null;\n    }\n\n    /** ‚úÖ STAFF METHODS **/\n  }\n};","map":{"version":3,"names":["axios","mapGetters","name","data","form","accountName","accountNumber","bankName","banks","isEditing","editId","mounted","fetchBanks","computed","methods","res","get","headers","Authorization","getToken","err","console","error","createBank","post","length","push","resetForm","alert","editBank","bank","_id","cancelEdit","updateBank","put","index","findIndex","b","splice","deleteBank","id","confirm","delete","filter"],"sources":["C:\\Users\\ajib\\OneDrive\\Desktop\\xASn2901-Daredevil\\event-ticket-frontend\\src\\components\\app\\bank.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-4\">\r\n    <!-- ‚úÖ Create or Edit Form -->\r\n    <div class=\"row\">\r\n      <div class=\"col-md-8\">\r\n        <form\r\n          @submit.prevent=\"isEditing ? updateBank() : createBank()\"\r\n          class=\"shadow p-4 w-md-75 mx-auto w-100\"\r\n        >\r\n          <h5>{{ isEditing ? \"‚úèÔ∏è Edit Bank Info\" : \"‚ûï Add Bank Info\" }}</h5>\r\n\r\n          <div class=\"mb-3\">\r\n            <label>Account Name</label>\r\n            <input v-model=\"form.accountName\" type=\"text\" class=\"form-control\" required />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label>Account Number</label>\r\n            <input\r\n              v-model=\"form.accountNumber\"\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              required\r\n            />\r\n          </div>\r\n\r\n          <div class=\"mb-3\">\r\n            <label>Bank Name</label>\r\n            <input v-model=\"form.bankName\" type=\"text\" class=\"form-control\" required />\r\n          </div>\r\n\r\n          <button type=\"submit\" class=\"btn btn-success\">\r\n            {{ isEditing ? \"Update\" : \"Create\" }}\r\n          </button>\r\n          <button\r\n            v-if=\"isEditing\"\r\n            type=\"button\"\r\n            class=\"btn btn-secondary ms-2\"\r\n            @click=\"cancelEdit\"\r\n          >\r\n            Cancel\r\n          </button>\r\n        </form>\r\n      </div>\r\n      <div class=\"text-start col-md-4\" v-if=\"banks && banks.length\">\r\n        <ul class=\"list-group shadow-sm\">\r\n          <li v-for=\"bank in banks\" :key=\"bank._id\" class=\"list-group-item\">\r\n            <div class=\"mb-2\">\r\n              <div><strong>Bank Account Name:</strong> {{ bank.accountName }}</div>\r\n              <div><strong>Account Number:</strong> {{ bank.accountNumber }}</div>\r\n              <div><strong>Bank Name:</strong> {{ bank.bankName }}</div>\r\n            </div>\r\n            <div class=\"d-flex gap-2\">\r\n              <button class=\"btn btn-sm btn-outline-warning\" @click=\"editBank(bank)\">\r\n                ‚úèÔ∏è Edit\r\n              </button>\r\n              <button class=\"btn btn-sm btn-outline-danger\" @click=\"deleteBank(bank._id)\">\r\n                üóëÔ∏è Delete\r\n              </button>\r\n            </div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"BankManager\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        accountName: \"\",\r\n        accountNumber: \"\",\r\n        bankName: \"\",\r\n      },\r\n      banks: [],\r\n\r\n      isEditing: false,\r\n      editId: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchBanks();\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getToken\"]),\r\n  },\r\n  methods: {\r\n    /** ‚úÖ BANK METHODS **/\r\n    async fetchBanks() {\r\n      try {\r\n        const res = await axios.get(\"https://event-ticket-backend-gnmw.onrender.com/api/bank\", {\r\n          headers: { Authorization: `Bearer ${this.getToken}` },\r\n        });\r\n        this.banks = res.data.banks;\r\n      } catch (err) {\r\n        console.error(\"Error fetching banks:\", err);\r\n      }\r\n    },\r\n    async createBank() {\r\n      try {\r\n        const res = await axios.post(\r\n          \"https://event-ticket-backend-gnmw.onrender.com/api/bank\",\r\n          this.form,\r\n          {\r\n            headers: { Authorization: `Bearer ${this.getToken}` },\r\n          }\r\n        );\r\n\r\n        if (res.data.banks && res.data.banks.length > 0) {\r\n          this.banks.push(res.data.banks[0]);\r\n        } else {\r\n          await this.fetchBanks();\r\n        }\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error creating bank:\", err);\r\n        alert(\"‚ùå Failed to create.\");\r\n      }\r\n    },\r\n    editBank(bank) {\r\n      this.isEditing = true;\r\n      this.editId = bank._id;\r\n      this.form = { ...bank };\r\n    },\r\n    cancelEdit() {\r\n      this.resetForm();\r\n    },\r\n    async updateBank() {\r\n      try {\r\n        const res = await axios.put(\r\n          `https://event-ticket-backend-gnmw.onrender.com/api/bank/${this.editId}`,\r\n          this.form,\r\n          {\r\n            headers: { Authorization: `Bearer ${this.getToken}` },\r\n          }\r\n        );\r\n        const index = this.banks.findIndex((b) => b._id === this.editId);\r\n        if (res.data.banks && res.data.banks.length > 0) {\r\n          this.banks.splice(index, 1, res.data.banks[0]);\r\n        } else {\r\n          await this.fetchBanks();\r\n        }\r\n        this.resetForm();\r\n      } catch (err) {\r\n        console.error(\"Error updating bank:\", err);\r\n      }\r\n    },\r\n    async deleteBank(id) {\r\n      if (!confirm(\"Delete this bank info?\")) return;\r\n      try {\r\n        await axios.delete(`https://event-ticket-backend-gnmw.onrender.com/api/bank/${id}`, {\r\n          headers: { Authorization: `Bearer ${this.getToken}` },\r\n        });\r\n        this.banks = this.banks.filter((b) => b._id !== id);\r\n      } catch (err) {\r\n        console.error(\"Error deleting bank:\", err);\r\n      }\r\n    },\r\n\r\n    resetForm() {\r\n      this.form = { accountName: \"\", accountNumber: \"\", bankName: \"\" };\r\n      this.isEditing = false;\r\n      this.editId = null;\r\n    },\r\n\r\n    /** ‚úÖ STAFF METHODS **/\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"mappings":";;;AAoEA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,WAAW,EAAE,EAAE;QACfC,aAAa,EAAE,EAAE;QACjBC,QAAQ,EAAE;MACZ,CAAC;MACDC,KAAK,EAAE,EAAE;MAETC,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGZ,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EACDa,OAAO,EAAE;IACP;IACA,MAAMF,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMG,GAAE,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,yDAAyD,EAAE;UACrFC,OAAO,EAAE;YAAEC,aAAa,EAAE,UAAU,IAAI,CAACC